    plt.title('Line Chart')
    plt.show()

def draw_figure_chart(stock_data):
    '''
    Draw a figure chart using the given stock data.
    '''
    plt.figure(figsize=(10, 6))
    plt.plot(stock_data.index, stock_data['Close'])
    plt.title('Figure Chart')
    plt.show()
