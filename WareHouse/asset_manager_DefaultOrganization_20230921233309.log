import streamlit as st
import yfinance as yf
import matplotlib.pyplot as plt
from mplfinance.original_flavor import candlestick_ohlc

def download_stock_data(stock_name, start_date, end_date):
    # Download historical stock data using yfinance library
    stock_data = yf.download(stock_name, start=start_date, end=end_date)
    return stock_data

def plot_candlestick_chart(stock_data):
    # Plot candlestick chart using mplfinance library
    fig, ax = plt.subplots()
    candlestick_ohlc(ax, stock_data[['Open', 'High', 'Low', 'Close']].values, width=0.6, colorup='green', colordown='red')
    ax.set_xlabel('Date')
    ax.set_ylabel('Price')
    ax.set_title('Candlestick Chart')
    plt.xticks(rotation=45)
    st.pyplot(fig)

def plot_bar_chart(stock_data):
    # Plot bar chart using matplotlib library
    fig, ax = plt.subplots()
    ax.bar(stock_data.index, stock_data['Volume'])
    ax.set_xlabel('Date')
    ax.set_ylabel('Volume')
    ax.set_title('Bar Chart')
    plt.xticks(rotation=45)
    st.pyplot(fig)

def plot_line_chart(stock_data):
    # Plot line chart using matplotlib library
    fig, ax = plt.subplots()
    ax.plot(stock_data.index, stock_data['Close'])
    ax.set_xlabel('Date')
    ax.set_ylabel('Price')
    ax.set_title('Line Chart')
    plt.xticks(rotation=45)
    st.pyplot(fig)

def main():
    st.set_page_config(layout='wide')
    st.title('Stock Analysis')
    
    stock_name = st.text_input('Enter stock name (e.g., Apple):')
    start_date = st.date_input('Enter start date:')
    end_date = st.date_input('Enter end date:')
    
    if st.button('Download and Plot'):
        stock_data = download_stock_data(stock_name, start_date, end_date)
        
        if not stock_data.empty:
            st.subheader('Candlestick Chart')
            plot_candlestick_chart(stock_data)
            
            st.subheader('Bar Chart')
            plot_bar_chart(stock_data)
            
            st.subheader('Line Chart')
            plot_line_chart(stock_data)
        else:
            st.error('No data available for the selected stock and date range.')

if __name__ == '__main__':
    main()
